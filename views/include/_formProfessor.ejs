<!-- form handling-->
<form action="/api/professors" method="POST" id="add_professor">
    <div class="new_user">
        <div class="form-group">
            <label for="professorID" class="text-light">professorID</label>
            <input type="hidden" name="id" value="">
            <input type="text" name="professorID" value="" placeholder="201968063">
        </div>
        <div class="form-group">
            <label for="Password" class="text-light">Password</label>
            <input type="hidden" name="id" value="">
            <input type="text" name="password" value="">
        </div>
        <div class="form-group">
            <label for="Name" class="text-light">Name</label>
            <input type="text" name="name" value="">
        </div>
        <div class="form-group">
            <label for="Email" class="text-light">Email</label>
            <input type="text" name="email" value="" placeholder="example@gmail.com">
        </div>
        <div class="form-group">
            <label for="Phone number" class="text-light">Phone Number</label>
            <input type="text" name="phoneNumber" value="" placeholder="+821099999999">
        </div>
     

        <div class="form-group">
            <button type="submit" class="btn text-dark update">Save</button>
        </div>
    </div>
</form>