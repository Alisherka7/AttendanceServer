<!-- form handling-->
<form action="/api/users" method="POST" id="add_user">
    <div class="new_user">
        <div class="form-group">
            <label for="StudentID" class="text-light">StudentID</label>
            <input type="hidden" name="id" value="">
            <input type="text" name="studentID" value="" placeholder="201968063">
        </div>
        <div class="form-group">
            <label for="Password" class="text-light">Password</label>
            <input type="hidden" name="id" value="">
            <input type="text" name="password" value="">
        </div>
        <div class="form-group">
            <label for="Name" class="text-light">Name</label>
            <input type="text" name="name" value="">
        </div>
        <div class="form-group">
            <label for="Email" class="text-light">Email</label>
            <input type="text" name="email" value="" placeholder="example@gmail.com">
        </div>
        <div class="form-group">
            <label for="Phone number" class="text-light">Phone Number</label>
            <input type="text" name="phoneNumber" value="" placeholder="+821099999999">
        </div>
        <div class="form-group">
            <label for="Lectures" class="text-light">Lectures</label>
            <% for(var i = 0; i<lectures.length; i++){ %><br>
                <%= lectures[i].lecture %><input type="checkbox" id="<%= lectures[i]._id %>" name="<%= lectures[i].lecture %>" value="sef" placeholder="">
            
                <% } %>
          
        </div>

        <div class="form-group">
            <button type="submit" class="btn text-dark update">Save</button>
        </div>
    </div>
</form>