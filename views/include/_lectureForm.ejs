<!-- form handling-->
<form action="/api/lectures" method="POST" id="add_user">
    <div class="new_user">
        <div class="form-group">
            <label for="Lecture" class="text-light">Lecture</label>
            <input type="hidden" name="id" value="">
            <input type="text" name="lecture" value="">
        </div>
        <div class="form-group">
            <label for="professorName" class="text-light">Professor Name</label>
            <input type="text" name="professorName" value="">
        </div>
        <div class="form-group">
            <label for="lectureRoom" class="text-light">Lecture room</label>
            <input type="text" name="lectureRoom" value="">
        </div>
        <div class="form-group">
            <label for="phoneNumber" class="text-light">Phone Number</label>
            <input type="text" name="phoneNumber" value="">
        </div>
        <div class="form-group">
            <label for="time" class="text-light">Time</label>
            <input type="text" name="time" value="">
        </div>

        <div class="form-group">
            <button type="submit" class="btn text-dark update">Save</button>
        </div>
    </div>
</form>